{
    "$schema": "http://json-schema.org/schema",
    "type": "object",
    "required": [
        "playlists"
    ],
    "properties": {
        "$schema": {
            "type": "string"
        },
        "playlists": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "presets"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "presets": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "textCuelist": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "templateId",
                    "activeStyle",
                    "inactiveStyle",
                    "texts"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "templateId": {
                        "type": "string"
                    },
                    "activeStyle": {
                        "type": "object"
                    },
                    "inactiveStyle": {
                        "type": "object"
                    },
                    "texts": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}