{
    "$schema": "http://json-schema.org/schema",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "presets": {
            "type": "array",
            "items": {
                "required": [
                    "id"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "styles": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "required": [
                                "id",
                                "style"
                            ],
                            "properties": {
                                "id": {
                                    "type": "string"
                                },
                                "style": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "text": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "required": [
                                "key",
                                "value"
                            ],
                            "properties": {
                                "key": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}